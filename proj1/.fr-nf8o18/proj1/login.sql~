drop table login;
drop table pictures;
create table login(id char(20), pwd char(20));
CREATE TABLE pictures (pic_id int, pic_desc  varchar(100), pic  BLOB, primary key(pic_id));
CREATE SEQUENCE pic_id_sequence;

